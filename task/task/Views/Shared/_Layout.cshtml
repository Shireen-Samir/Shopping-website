<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>@ViewBag.Title - My ASP.NET Application</title>
    @Styles.Render("~/Content/css") 
    @Scripts.Render("~/bundles/modernizr")

     
     
    <title>Winkel - Free Bootstrap 4 Template by Colorlib</title>
    <meta charset="utf-8">
    <meta name="viewport" content="width=device-width, initial-scale=1, shrink-to-fit=no">
    
    <link href="https://fonts.googleapis.com/css?family=Open+Sans:300,400,600,700,800" rel="stylesheet">

    <link rel="stylesheet" href=@Url.Content("/Content/Templete/css/open-iconic-bootstrap.min.css")>
    <link rel="stylesheet" href=@Url.Content("/Content/Templete/css/animate.css")>
    
    <link rel="stylesheet" href=@Url.Content("/Content/Templete/css/owl.carousel.min.css")>
    <link rel="stylesheet" href=@Url.Content("/Content/Templete/css/owl.theme.default.min.css")>
    <link rel="stylesheet" href=@Url.Content("/Content/Templete/css/magnific-popup.css")>

    <link rel="stylesheet" href=@Url.Content("/Content/Templete/css/aos.css")>

    <link rel="stylesheet" href=@Url.Content("/Content/Templete/css/ionicons.min.css")>

    <link rel="stylesheet" href=@Url.Content("/Content/Templete/css/bootstrap-datepicker.css")>
    <link rel="stylesheet" href=@Url.Content("/Content/Templete/css/jquery.timepicker.css")>

    
    <link rel="stylesheet" href=@Url.Content("/Content/Templete/css/flaticon.css")>
    <link rel="stylesheet" href=@Url.Content("/Content/Templete/css/icomoon.css")>
    <link rel="stylesheet" href=@Url.Content("/Content/Templete/css/style.css")>
  

</head>

<body class="goto-here" style="padding:0px">
    <div class="py-1 bg-black"style="height:31px" >
        <div class="container">
            <div class="row no-gutters d-flex align-items-start align-items-center px-md-0">
                <div class="col-lg-12 d-block">
                    <div class="row d-flex" style="margin-top:1px">
                        <div class="col-md pr-4 d-flex topper align-items-center">
                            <div class="icon mr-2 d-flex justify-content-center align-items-center"><span class="icon-phone2"></span></div>
                            <span class="text" style="font-size:12px">+ 1235 2355 98</span>
                        </div>
                        <div class="col-md pr-4 d-flex topper align-items-center">
                            <div class="icon mr-2 d-flex justify-content-center align-items-center"><span class="icon-paper-plane"></span></div>
                            <span class="text"style="font-size:12px">youremail@email.com</span>
                        </div>
                        <div class="col-md-5 pr-4 d-flex topper align-items-center text-lg-right">
                            <span class="text"style="font-size:12px">3-5 Business days delivery &amp; Free Returns</span>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </div>
    <nav class="navbar navbar-expand-lg navbar-dark ftco_navbar bg-dark ftco-navbar-light" id="ftco-navbar">
        @*<div class="container">*@
            <a class="navbar-brand" href="index.html">magyAccessories</a>
            <button class="navbar-toggler" type="button" data-toggle="collapse" data-target="#ftco-nav" aria-controls="ftco-nav" aria-expanded="false" aria-label="Toggle navigation">
                <span class="oi oi-menu"></span> Menu
            </button>

            <div class="collapse navbar-collapse" id="ftco-nav">
                <ul class="navbar-nav ml-auto" style="margin-right:8px">
                    <li class="nav-item">
                        @Html.ActionLink("Home", "Index", "Home", null, new { @class = "nav-link" ,@style = "font-size:14px"})

                    </li>
                    &nbsp;
                    &nbsp;
                    <li class="nav-item">
                        @Html.ActionLink("Proudect", "Index", "Proudects", null, new { @class = "nav-link" ,@style = "font-size:14px"})
                    </li>
                    &nbsp;
                    &nbsp;
                    <li class="nav-item ">
                        @Html.ActionLink("Category", "Index", "Categories", null, new { @class = "nav-link" ,@style = "font-size:14px"})
                    </li>

                    <li class="nav-item dropdown">
                        <a class="nav-link dropdown-toggle" href="#" id="dropdown04" data-toggle="dropdown" aria-haspopup="true" aria-expanded="false"style="font-size:14px">Shop</a>
                        <div class="dropdown-menu" aria-labelledby="dropdown04">
                            @*<a class="dropdown-item" href=@Url.Content("/Content/Templete/shop.html")>Shop</a>*@
                            @*<a class="dropdown-item" href=@Url.Content("/Content/Templete/product-single.html")>Single Product</a>*@
                            <a class="dropdown-item" href=@Url.Content("/Content/Templete/cart.html")>Cart</a>
                            <a class="dropdown-item" href=@Url.Content("/Content/Templete/checkout.html")>Checkout</a>
                        </div>
                    </li>



                    <li class="nav-item"><a href=@Url.Content("/Content/Templete/about.html") class="nav-link" style="font-size:14px">About</a></li>
                    <li class="nav-item"><a href=@Url.Content("/Content/Templete/blog.html") class="nav-link" style="font-size:14px">Blog</a></li>
                    <li class="nav-item"><a href=@Url.Content("/Content/Templete/contact.html") class="nav-link" style="font-size:14px">Contact</a></li>
                    <li class="nav-item cta cta-colored"><a href="cart.html" class="nav-link"><span class="icon-shopping_cart" style="width:20px"></span>[0]</a></li>


                </ul>
                @Html.Partial("_LoginPartial")
            </div>
        @*</div>*@
    </nav>
    <!-- END nav -->

    @RenderBody()

    <!-- loader -->
    <div id="ftco-loader" class="show fullscreen"><svg class="circular" width="48px" height="48px"><circle class="path-bg" cx="24" cy="24" r="22" fill="none" stroke-width="4" stroke="#eeeeee" /><circle class="path" cx="24" cy="24" r="22" fill="none" stroke-width="4" stroke-miterlimit="10" stroke="#F96D00" /></svg></div>


    <script src="~/Content/Templete/js/jquery.min.js"></script>
    <script src="~/Content/Templete/js/jquery-migrate-3.0.1.min.js"></script>
    <script src="~/Content/Templete/js/popper.min.js"></script>
    <script src="~/Content/Templete/js/bootstrap.min.js"></script>
    <script src="~/Content/Templete/js/jquery.easing.1.3.js"></script>
    <script src="~/Content/Templete/js/jquery.waypoints.min.js"></script>
    <script src="~/Content/Templete/js/jquery.stellar.min.js"></script>
    <script src="~/Content/Templete/js/owl.carousel.min.js"></script>
    <script src="~/Content/Templete/js/jquery.magnific-popup.min.js"></script>
    <script src="~/Content/Templete/js/aos.js"></script>
    <script src="~/Content/Templete/js/jquery.animateNumber.min.js"></script>
    <script src="~/Content/Templete/js/bootstrap-datepicker.js"></script>
    <script src="~/Content/Templete/js/scrollax.min.js"></script>
    <script src="https://maps.googleapis.com/maps/api/js?key=AIzaSyBVWaKrjvy3MaE7SQ74_uJiULgl1JY0H2s&sensor=false"></script>
    <script src="~/Content/Templete/js/google-map.js"></script>
    <script src="~/Content/Templete/js/main.js"></script>


    <hr style="margin-top:57px"/>
  

    @Scripts.Render("~/bundles/jquery")
    @Scripts.Render("~/bundles/bootstrap")
    @RenderSection("scripts", required: false)
</body>
</html>
